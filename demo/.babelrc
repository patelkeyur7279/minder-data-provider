{
  "presets": [
    [
      "next/babel",
      {
        "preset-react": {
          "runtime": "automatic",
          "importSource": "react"
        }
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-transform-class-static-block",
    [
      "@babel/plugin-transform-runtime",
      {
        "regenerator": true,
        "helpers": true,
        "useESModules": true
      }
    ]
  ]
}