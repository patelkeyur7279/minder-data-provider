{
  "name": "minder-data-provider",
  "version": "2.0.0",
  "description": "Production-ready hybrid Redux + TanStack Query data provider for Next.js with CORS support, WebSocket, and advanced caching",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./core/*": {
      "types": "./dist/core/*.d.ts",
      "import": "./dist/core/*.js",
      "require": "./dist/core/*.cjs"
    },
    "./hooks/*": {
      "types": "./dist/hooks/*.d.ts",
      "import": "./dist/hooks/*.js",
      "require": "./dist/hooks/*.cjs"
    },
    "./crud": {
      "types": "./dist/crud/index.d.ts",
      "import": "./dist/crud/index.js",
      "require": "./dist/crud/index.cjs"
    },
    "./auth": {
      "types": "./dist/auth/index.d.ts",
      "import": "./dist/auth/index.js",
      "require": "./dist/auth/index.cjs"
    },
    "./cache": {
      "types": "./dist/cache/index.d.ts",
      "import": "./dist/cache/index.js",
      "require": "./dist/cache/index.cjs"
    },
    "./websocket": {
      "types": "./dist/websocket/index.d.ts",
      "import": "./dist/websocket/index.js",
      "require": "./dist/websocket/index.cjs"
    },
    "./upload": {
      "types": "./dist/upload/index.d.ts",
      "import": "./dist/upload/index.js",
      "require": "./dist/upload/index.cjs"
    },
    "./debug": {
      "types": "./dist/debug/index.d.ts",
      "import": "./dist/debug/index.js",
      "require": "./dist/debug/index.cjs"
    },
    "./config": {
      "types": "./dist/config/index.d.ts",
      "import": "./dist/config/index.js",
      "require": "./dist/config/index.cjs"
    },
    "./ssr": {
      "types": "./dist/ssr/index.d.ts",
      "import": "./dist/ssr/index.js",
      "require": "./dist/ssr/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build && npm run test",
    "demo": "cd demo && npm run dev",
    "demo:build": "cd demo && npm run build",
    "start": "npm run demo",
    "analyze": "npm run build && npx bundlesize",
    "security-audit": "npm audit --audit-level moderate"
  },
  "keywords": [
    "react",
    "redux",
    "tanstack-query",
    "nextjs",
    "data-provider",
    "crud",
    "optimistic-updates",
    "typescript",
    "cors",
    "websocket",
    "caching",
    "state-management"
  ],
  "author": "Minder Data Provider Team",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/plugin-transform-class-static-block": "^7.28.3",
    "@reduxjs/toolkit": "^2.3.0",
    "@tanstack/react-query": "^5.59.20",
    "@tanstack/react-query-devtools": "^5.59.20",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.0.0",
    "@types/cors": "^2.8.17",
    "@types/dompurify": "^3.0.5",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.73",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "axios": "^1.7.7",
    "dompurify": "^3.3.0",
    "eslint": "^8.57.0",
    "http-server": "^14.1.1",
    "immer": "^10.1.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "react-redux": "^9.1.2",
    "ts-jest": "^29.1.2",
    "tsup": "^8.0.2",
    "typescript": "^5.4.3"
  },
  "peerDependencies": {
    "@reduxjs/toolkit": "^2.0.0",
    "@tanstack/react-query": "^5.0.0",
    "axios": "^1.0.0",
    "immer": "^10.0.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "react-redux": "^9.0.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/demo/",
      "/dist/"
    ],
    "preset": "ts-jest",
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "transform": {
      "^.+\\.(ts|tsx)$": [
        "ts-jest",
        {
          "tsconfig": {
            "jsx": "react",
            "esModuleInterop": true,
            "allowSyntheticDefaultImports": true
          }
        }
      ]
    },
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts",
      "!src/**/*.test.{ts,tsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/minder-data-provider/minder-data-provider.git"
  },
  "bugs": {
    "url": "https://github.com/minder-data-provider/minder-data-provider/issues"
  },
  "homepage": "https://github.com/minder-data-provider/minder-data-provider#readme"
}
